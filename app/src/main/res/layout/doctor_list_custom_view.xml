<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent" android:layout_height="match_parent">

    <TextView
        android:id="@+id/doctor_name_doctor_list_custom"
        android:layout_width="271dp"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="131dp"
        android:layout_marginTop="40dp"
        android:fontFamily="sans-serif-black"
        android:text="TextView"
        android:textSize="20sp"
        android:focusableInTouchMode="false"
        android:focusable="false" />

    <ImageView
        android:id="@+id/image_view_doctor_list_custom"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:scaleType="fitXY"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="37dp"
        android:layout_marginTop="34dp"
        app:srcCompat="@mipmap/ic_launcher"
        android:focusableInTouchMode="false"
        android:focusable="false" />

    <TextView
        android:id="@+id/doctor_titile_doctor_list_custom"
        android:layout_width="259dp"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="124dp"
        android:layout_marginTop="81dp"
        android:text="TextView"
        android:textAlignment="center"
        android:textSize="16sp"
        android:focusableInTouchMode="false"
        android:focusable="false" />

    <TextView
        android:id="@+id/btn_book_doctor_list_custom"
        android:layout_width="224dp"
        android:layout_height="40dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="93dp"
        android:layout_marginTop="130dp"
        android:layout_marginBottom="28dp"
        android:background="@drawable/round_button_submit"
        android:focusable="false"
        android:focusableInTouchMode="false"
        android:fontFamily="sans-serif-black"
        android:paddingTop="10dp"
        android:text="Book Now"
        android:textAlignment="center"
        android:textColor="@android:color/black"
        android:textSize="16sp" />
</RelativeLayout>